{
    "manifest_version": 2,
    "name": "Discordify",
    "version": "1.0",

    "description": "Discord grid for talks",
    "icons": {
        "48": "icons/shelf-48.png"
    },

    "applications": {
        "gecko": {
            "id": "borderify@mozilla.org",
            "strict_min_version": "45.0"
        }
    },

    "content_scripts": [
        {
            "matches": ["https://discord.com/*"],
            "js": ["actions.js"]
        }
    ]

}

